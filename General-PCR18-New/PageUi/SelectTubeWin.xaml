<Window x:Class="General_PCR18.PageUi.SelectTubeWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:General_PCR18.PageUi"
        mc:Ignorable="d"
        Title="Select Tube" Height="300" Width="480" ShowInTaskbar="False" BorderThickness="1,0,1,1" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="1" Margin="10">
            <CheckBox Content="全选" Click="CheckBox_Click"></CheckBox>
        </StackPanel>
        <WrapPanel Grid.Row="2" x:Name="panelTube" Margin="10">
            
        </WrapPanel>
        <StackPanel Grid.Row="3">
            <Button Content="OK" Width="100" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
